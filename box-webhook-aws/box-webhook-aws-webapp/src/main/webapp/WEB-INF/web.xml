<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app metadata-complete="true"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
        version="2.5">
    <display-name>Webhook Servlet Example</display-name>

    <!-- Servlets -->
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.box.sdk.webhookawssample.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignIn</servlet-name>
        <servlet-class>com.box.sdk.webhookawssample.SignInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateUser</servlet-name>
        <servlet-class>com.box.sdk.webhookawssample.CreateUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dashboard</servlet-name>
        <servlet-class>com.box.sdk.webhookawssample.DashboardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DocDetails</servlet-name>
        <servlet-class>com.box.sdk.webhookawssample.DocDetailsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Download</servlet-name>
        <servlet-class>com.box.sdk.webhookawssample.DownloadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Thumbnail</servlet-name>
        <servlet-class>com.box.sdk.webhookawssample.ThumbnailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Preview</servlet-name>
        <servlet-class>com.box.sdk.webhookawssample.PreviewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NotifyOnPreview</servlet-name>
        <servlet-class>com.box.sdk.webhookawssample.NotifyOnPreviewServlet</servlet-class>
    </servlet>
    <!-- Servlet Mappings -->
    <servlet-mapping>
        <servlet-name>Dashboard</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DocDetails</servlet-name>
        <url-pattern>/docdetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Download</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Thumbnail</servlet-name>
        <url-pattern>/thumbnail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Preview</servlet-name>
        <url-pattern>/preview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignIn</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateUser</servlet-name>
        <url-pattern>/createuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NotifyOnPreview</servlet-name>
        <url-pattern>/notify-on-preview</url-pattern>
    </servlet-mapping>
</web-app>
